@inject ClientModel ClientModel

<MudNavMenu>
    <MudNavLink Href="/">Home</MudNavLink>
    <MudNavGroup Title="Queries" Expanded="false">
        @if (ClientModel.QueryMetaDatas != null)
        {
            @foreach (var queryMetaData in ClientModel.QueryMetaDatas)
            {
                <MudNavLink Href=@("showquery/" + queryMetaData.Name)>@queryMetaData.Label</MudNavLink>
            }
        }
     </MudNavGroup>
    <MudNavLink Href="/queryadmin">Query Administration</MudNavLink>
</MudNavMenu>

